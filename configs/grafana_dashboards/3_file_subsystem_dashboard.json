{
  "dashboard": {
    "title": "File Manager Details",
    "description": "File Manager performance, throughput, and resource utilization",
    "tags": ["gaia", "file", "subsystem"],
    "timezone": "browser",
    "panels": [
      {
        "title": "File Operation Success Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "(gaia_file_success_total / gaia_file_operations_total) * 100"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          }
        }
      },
      {
        "title": "File Operation Error Rate",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "(gaia_file_errors_total / gaia_file_operations_total) * 100"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      {
        "title": "File P95 Latency (ms)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, gaia_file_latency_ms)"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 250},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      {
        "title": "Concurrent Operations",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "gaia_file_concurrent_operations"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      {
        "title": "Operations Rate (24h)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "rate(gaia_file_operations_total[5m])",
            "legendFormat": "Ops/sec"
          }
        ]
      },
      {
        "title": "Success vs Failure Rate (24h)",
        "type": "timeseries",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "rate(gaia_file_success_total[5m])",
            "legendFormat": "Success/sec"
          },
          {
            "expr": "rate(gaia_file_errors_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ]
      },
      {
        "title": "Bytes Processed (24h)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "increase(gaia_file_bytes_processed_total[24h])"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "unit": "bytes"
        }
      },
      {
        "title": "Throughput (MB/s) (24h)",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "rate(gaia_file_bytes_processed_total[5m]) / 1024 / 1024",
            "legendFormat": "MB/sec"
          }
        ]
      },
      {
        "title": "Latency Percentiles (24h)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, gaia_file_latency_ms)",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, gaia_file_latency_ms)",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, gaia_file_latency_ms)",
            "legendFormat": "P99"
          }
        ]
      },
      {
        "title": "Concurrent Operations Trend (24h)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "gaia_file_concurrent_operations",
            "legendFormat": "Concurrent Ops"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      {
        "title": "Total Operations Summary (24h)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 24, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "increase(gaia_file_operations_total[24h])"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "title": "Success Operations Summary (24h)",
        "type": "stat",
        "gridPos": {"x": 8, "y": 24, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "increase(gaia_file_success_total[24h])"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "title": "Failed Operations Summary (24h)",
        "type": "stat",
        "gridPos": {"x": 16, "y": 24, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "increase(gaia_file_errors_total[24h])"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 37,
    "style": "dark",
    "time": {
      "from": "now-24h",
      "to": "now"
    }
  },
  "overwrite": true
}
