{
  "dashboard": {
    "title": "Process Manager Details",
    "description": "Process Manager resource utilization, execution, and health",
    "tags": ["gaia", "process", "subsystem"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Active Processes",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "gaia_process_active"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 75},
              {"color": "red", "value": 150}
            ]
          }
        }
      },
      {
        "title": "Process Memory (MB)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "gaia_process_memory_mb"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 250},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      {
        "title": "Process CPU Usage (%)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "gaia_process_cpu_percent"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 80}
            ]
          }
        }
      },
      {
        "title": "Process Success Rate",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "(gaia_process_completed_total / (gaia_process_started_total + 1)) * 100"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "gauge",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 90},
              {"color": "green", "value": 95}
            ]
          }
        }
      },
      {
        "title": "Active Processes Trend (24h)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "gaia_process_active",
            "legendFormat": "Active Processes"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 75},
              {"color": "red", "value": 150}
            ]
          }
        }
      },
      {
        "title": "Process Completion Rate (24h)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(gaia_process_completed_total[5m])",
            "legendFormat": "Completed/sec"
          },
          {
            "expr": "rate(gaia_process_failed_total[5m])",
            "legendFormat": "Failed/sec"
          }
        ]
      },
      {
        "title": "Memory Trend (24h)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "gaia_process_memory_mb",
            "legendFormat": "Memory (MB)"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 250},
              {"color": "red", "value": 500}
            ]
          }
        }
      },
      {
        "title": "CPU Usage Trend (24h)",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "gaia_process_cpu_percent",
            "legendFormat": "CPU %"
          }
        ],
        "options": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 80}
            ]
          }
        }
      },
      {
        "title": "Processes Started (24h)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 24, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "increase(gaia_process_started_total[24h])"
          }
        ]
      },
      {
        "title": "Processes Completed (24h)",
        "type": "stat",
        "gridPos": {"x": 8, "y": 24, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "increase(gaia_process_completed_total[24h])"
          }
        ]
      },
      {
        "title": "Processes Failed (24h)",
        "type": "stat",
        "gridPos": {"x": 16, "y": 24, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "increase(gaia_process_failed_total[24h])"
          }
        ]
      }
    ],
    "refresh": "30s",
    "schemaVersion": 37,
    "style": "dark",
    "time": {
      "from": "now-24h",
      "to": "now"
    }
  },
  "overwrite": true
}
